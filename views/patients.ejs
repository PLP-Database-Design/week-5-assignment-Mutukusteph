<table>
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date of Birth</th>
        </tr>
    </thead>
    <tbody>
        <% if (results.length > 0) { %>
            <% results.forEach(row => { %>
                <tr>
                    <td><%= row.patient_id %></td>
                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    <td><%= row.date_of_birth %></td>
                </tr>
            <% }); %>
        <% } else { %>
            <tr>
                <td colspan="4">No patients found.</td>
            </tr>
        <% } %>
    </tbody>
</table>

